{
  "name": "CommandCenter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android:dev": "ENVFILE=.env.development react-native run-android --variant=developmentdebugDebug",
    "android:dev-release": "ENVFILE=.env.development react-native run-android --variant=developmentreleaseRelease",
    "android:prod": "ENVFILE=.env.production react-native run-android --variant=productiondebugDebug",
    "android:prod-release": "ENVFILE=.env.production react-native run-android --variant=productionreleaseRelease",
    "ios:dev": "ENVFILE=.env.staging react-native run-ios --scheme \"CommandCenterDev\" --configuration \"Debug\" --simulator='iPhone 14 Plus'",
    "ios:dev-release": "ENVFILE=.env.staging react-native run-ios --scheme \"CommandCenterDev\" --configuration \"Release\" --simulator='iPhone 14 Plus'",
    "ios:prod": "ENVFILE=.env.production react-native run-ios --scheme \"CommandCenterProd\" --configuration \"Debug\" --simulator='iPhone 14 Plus'",
    "ios:prod-release": "ENVFILE=.env.production react-native run-ios --scheme \"CommandCenterProd\" --configuration \"Release\" --simulator='iPhone 14 Plus'"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-config": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
